Algoritmo Ejercicio_3_8
	Definir A, B, C Como Real;
	A <- 4;
	B <- 5;
	C <- 1;
	// a)
	Escribir "a) El resultado es:  ", (B * A - B^2) / 4 * C;
	// b)
	Escribir "b) El resulado es:  ", (A * B) / 3^2;
	// c)
	Escribir "c) El resultado es:  ", (((B + C) / (2 * A + 10)) * 3 * B) - 6;
